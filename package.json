{
  "name": "babel-plugin-react-native-style",
  "version": "1.0.3",
  "description": "Improve the writing stylesheet experience",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/taixw2/babel-plugin-react-native-style"
  },
  "scripts": {
    "start": "yarn test --watch",
    "test": "jest",
    "cache:clear": "jest --clearCache",
    "build": "babel -d ./dist ./src -s",
    "coveralls": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "author": "fex@0000o.net",
  "license": "MIT",
  "keywords": [
    "babel",
    "plugin",
    "react-native",
    "react",
    "style",
    "react-native-style",
    "stylesheet",
    "react-native-stylesheet"
  ],
  "jest": {
    "testURL": "http://localhost/",
    "testRegex": "test/.*(test|spec).js?$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-tester": "^5.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^4.0.0",
    "fela": "^6.1.3",
    "istanbul": "^0.4.5",
    "jest": "^22.1.1",
    "prettier": "^1.10.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fela": "^6.2.4"
  },
  "dependencies": {
    "babel-plugin-react-native-style-convert-plugin": "^2.3.1",
    "babel-types": "^6.26.0",
    "lodash": "^4.17.11"
  },
  "peerDependencies": {
    "react-native": "^0.50.0",
    "babel-plugin-react-native-style-runtime": "~1.0.0"
  }
}
